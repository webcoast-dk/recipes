{
    "name": "webcoast/recipes",
    "description": "TYPO3 extension for managing recipes with ingredients and preparation",
    "type": "typo3-cms-extension",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "typo3/cms": {
            "extension-key": "recipes"
        }
    },
    "autoload": {
        "psr-4": {
            "WEBcoast\\Recipes\\": "Classes/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-intl": "*",
        "typo3/cms-core": "^v12.4",
        "typo3/cms-extbase": "^v12.4",
        "typo3/cms-fluid": "^v12.4"
    },
    "suggest": {
        "georgringer/numbered-pagination": "^1.0.4"
    }
}
